{
  "name": "vemn-todo",
  "version": "0.0.1",
  "description": "VEMN Todo App",
  "private": true,
  "scripts": {
    "clean": "rm -rf output",
    "start": "nodemon app.js --watch app.js --watch api --ignore api/views/ --exec babel-node --presets es2015",
    "build": "npm run clean & webpack --progress --colors",
    "pub": "cross-env NODE_ENV=production npm run build",
    "server": "webpack-dev-server --progress --inline --hot --content-base client/",
    "lint": "fecs -v && fecs ./**/*.js --type=js --rule",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "mongoose": "^4.3.7",
    "morgan": "~1.6.1",
    "readline": "^1.3.0",
    "serve-favicon": "~2.3.0",
    "vue": "^1.0.15",
    "vue-resource": "^0.7.0",
    "vue-router": "^0.7.11"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.9.0",
    "browser-sync": "^2.16.0",
    "cross-env": "^2.0.0",
    "css-loader": "^0.16.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "fecs-loader": "^0.1.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.17.0",
    "http-proxy": "^1.13.2",
    "json-loader": "^0.5.4",
    "less": "^2.6.0",
    "less-loader": "^2.2.2",
    "nodemon": "^1.10.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "vue-hot-reload-api": "^1.3.2",
    "vue-html-loader": "^1.2.0",
    "vue-loader": "^8.1.4",
    "vue-style-loader": "^1.0.0",
    "webpack": "^1.12.14",
    "webpack-config": "^0.8.1",
    "webpack-dev-server": "^1.14.1"
  }
}
